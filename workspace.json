{
  "version": 1,
  "projects": {
    "my-app": {
      "root": "apps/my-app",
      "sourceRoot": "apps/my-app/src",
      "projectType": "application",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@nrwl/web:build",
          "options": {
            "outputPath": "dist/apps/my-app",
            "index": "apps/my-app/public/index.html",
            "main": "apps/my-app/src/index.tsx",
            "polyfills": "apps/my-app/src/polyfills.ts",
            "tsConfig": "apps/my-app/tsconfig.app.json",
            "assets": [
              "apps/my-app/src/assets",
              "apps/my-app/src/manifest.json"
            ],
            "styles": ["apps/my-app/src/styles.css"],
            "scripts": [],
            "webpackConfig": "@nxtend/ionic-react/plugins/webpack"
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/my-app/src/environments/environment.ts",
                  "with": "apps/my-app/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@nrwl/web:dev-server",
          "options": {
            "buildTarget": "my-app:build"
          },
          "configurations": {
            "production": {
              "buildTarget": "my-app:build:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/my-app/**/*.{ts,tsx,js,jsx}"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/my-app/jest.config.js",
            "passWithNoTests": true
          }
        },
        "add": {
          "builder": "@nxtend/capacitor:add"
        },
        "copy": {
          "builder": "@nxtend/capacitor:copy"
        },
        "open": {
          "builder": "@nxtend/capacitor:open"
        },
        "sync": {
          "builder": "@nxtend/capacitor:sync"
        },
        "update": {
          "builder": "@nxtend/capacitor:update"
        }
      }
    },
    "my-app-e2e": {
      "root": "apps/my-app-e2e",
      "sourceRoot": "apps/my-app-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/my-app-e2e/cypress.json",
            "tsConfig": "apps/my-app-e2e/tsconfig.e2e.json",
            "devServerTarget": "my-app:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "my-app:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/my-app-e2e/**/*.{js,ts}"]
          }
        }
      }
    },
    "my-app-cap": {
      "projectType": "application",
      "root": "apps/my-app-cap",
      "sourceRoot": "apps/my-app-cap/src",
      "architect": {
        "add": {
          "builder": "@nxtend/capacitor:add"
        },
        "copy": {
          "builder": "@nxtend/capacitor:copy"
        },
        "open": {
          "builder": "@nxtend/capacitor:open"
        },
        "sync": {
          "builder": "@nxtend/capacitor:sync"
        },
        "update": {
          "builder": "@nxtend/capacitor:update"
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nxtend/ionic-react"
  },
  "schematics": {
    "@nrwl/react": {
      "application": {
        "babel": true
      }
    },
    "@nxtend/ionic-react": {
      "application": {
        "style": "css",
        "linter": "eslint"
      },
      "component": {
        "style": "css"
      },
      "library": {
        "style": "css",
        "linter": "eslint"
      }
    }
  },
  "defaultProject": "my-app"
}
